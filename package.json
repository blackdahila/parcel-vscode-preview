{
  "name": "parcel-vscode-preview",
  "displayName": "Parcel Preview",
  "description": "Get CodeSandbox Current Module View inside of your VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:currentModulePreview.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "currentModulePreview.start",
        "title": "Preview Current Module ðŸ“ƒðŸ‘€"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
