    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "@types/parcel-bundler": "^1.10.1",
    "@types/slash": "^2.0.0",
    "tslint": "^5.8.0",
    "typescript": "^3.1.4"
  },
  "dependencies": {
    "tagged-template-noop": "^2.1.0",
